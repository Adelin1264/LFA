    0
    1
States :
    q0
    q1
    q2
    q3
    q4
    q5
Start :
    q0
Final :
    q1
    q2
Transitions :
    q0, *, q1 q2
    q1, 0, q3
    q1, 1, q3
    q3, 0, q1
    q3, 1, q1
    q2, 0, q4
    q2, 1, q4
    q4, 0, q5
    q4, 1, q5
    q5, 0, q2
    q5, 1, q2